    "from scipy.optimize import fmin\n",
    "\n",
    "x = np.array([1,-3])\n",
    "def f1(x):\n",
    "    return(1.5*(x[0]**2) + x[1]**2 - 2*x[0]*x[1] + 2*x[0]**3  + 0.5*(x[0]**4))\n",
    "\n",
    "min = fmin(f1,x)\n",
    "print(\"Min:\",min)"
